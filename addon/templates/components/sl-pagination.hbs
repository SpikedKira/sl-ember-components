<li class="{{if onFirstPage "disabled"}}">
    <a {{action "previousPage"}} href="#" aria-label="Previous">
        <span class="sl-icon-previous">previous page</span>
    </a>
</li>

{{~#each range as |page|~}}
    <li class="{{if page.active "active"}}">
        <a href="#" {{action "gotoPage" page.index}}>{{page.index}}</a>
    </li>
{{~/each~}}

<li class="{{if onLastPage "disabled"}}">
    <a {{action "nextPage"}} href="#" aria-label="Next">
        <span class="sl-icon-next">next page</span>
    </a>
</li>
